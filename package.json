{
  "name": "@yellowgarbagebag/snow-white-api",
  "version": "1.0.1",
  "private": true,
  "description": "Snow White - REST API",
  "homepage": "https://gitlab.com/yellow-garbage-group/snow-white",
  "bugs": {
    "url": "https://gitlab.com/yellow-garbage-group/snow-white/issues"
  },
  "repository": {
    "type": "git",
    "url": "git@gitlab.com:yellow-garbage-group/snow-white.git"
  },
  "license": "MIT",
  "author": "Steffen",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "preinstall": "npx only-allow pnpm",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "prettier": "@hest-lab/eslint-config",
  "dependencies": {
    "@nestjs/common": "^7.6.18",
    "@nestjs/core": "^7.6.18",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/swagger": "4.8.2",
    "@prisma/client": "2.27.0-integration-fix-sdk-pnpm.1",
    "@yellowgarbagebag/snow-white-shared": "workspace:*",
    "axios": "0.21.1",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "dotenv": "8.6.0",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "mqtt": "4.2.8",
    "mysql": "2.18.1",
    "nodemailer": "6.6.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.7",
    "swagger-ui-express": "4.1.6",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@hest-lab/eslint-config": "1.8.0",
    "@nestjs/cli": "^7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "^7.6.18",
    "@types/express": "4.17.13",
    "@types/jest": "26.0.22",
    "@types/node": "^14.17.4",
    "@types/nodemailer": "6.4.4",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
    "eslint": "7.32.0",
    "jest": "^26.6.3",
    "prettier": "2.3.2",
    "prisma": "2.27.0-integration-fix-sdk-pnpm.1",
    "supertest": "6.1.4",
    "ts-jest": "26.5.6",
    "ts-loader": "8.3.0",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.10.1",
    "typescript": "4.3.5"
  },
  "engines": {
    "node": ">=14"
  }
}
