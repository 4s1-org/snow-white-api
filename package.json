{
  "name": "@yellowgarbagebag/snow-white-rest-api",
  "version": "1.0.1",
  "private": true,
  "description": "SnowWhite - REST API",
  "homepage": "https://gitlab.com/YellowGarbageBag/snow-white-rest-api",
  "bugs": {
    "url": "https://gitlab.com/YellowGarbageBag/snow-white-rest-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@gitlab.com:YellowGarbageBag/snow-white-rest-api.git"
  },
  "license": "ISC",
  "author": "Steffen",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint --ext .ts src/ test/",
    "lint:fix": "npm run lint -- --fix",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch"
  },
  "prettier": "@hest-lab/eslint-config",
  "jest": {
    "coverageDirectory": "../coverage",
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testEnvironment": "node",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    }
  },
  "dependencies": {
    "@nestjs/common": "^7.0.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/swagger": "^4.5.12",
    "@nestjs/typeorm": "^7.1.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "dotenv": "^8.2.0",
    "moment-timezone": "^0.5.31",
    "mysql": "^2.18.1",
    "nodemailer": "^6.4.10",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.25",
    "uuid": "^8.2.0"
  },
  "devDependencies": {
    "@hest-lab/eslint-config": "^1.2.0",
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/express": "^4.17.3",
    "@types/jest": "26.0.15",
    "@types/node": "^14.14.7",
    "@types/nodemailer": "^6.4.0",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.0.0",
    "@typescript-eslint/eslint-plugin": "^4.7.0",
    "@typescript-eslint/parser": "^4.7.0",
    "@yellowgarbagebag/snow-white-dto": "^2.0.1",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "prettier": "^2.1.2",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-loader": "^8.0.11",
    "ts-node": "9.0.0",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^4.0.5"
  }
}
