{
  "name": "@4s1/snow-white-rest-api",
  "version": "1.0.1",
  "private": true,
  "description": "Snow White - Rest API",
  "bugs": {
    "url": "https://gitlab.com/4s1/snow-white-rest-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/4s1/snow-white/snow-white-rest-api.git"
  },
  "license": "MIT",
  "author": "Steffen <steffen@4s1.de>",
  "type": "module",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "preinstall": "npx only-allow pnpm",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "prepare": "husky install",
    "prisma": "pnpm dlx prisma generate",
    "release": "git diff --exit-code --quiet && pnpm run lbt && standard-version",
    "release:major": "pnpm run release -- --release-as major",
    "release:minor": "pnpm run release -- --release-as minor",
    "release:patch": "pnpm run release -- --release-as patch",
    "start": "nest start",
    "start:debug": "nest start --debug --watch",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "test": "echo tests disabled # node --experimental-vm-modules node_modules/jest/bin/jest.js --passWithNoTests",
    "test:cov": "pnpm run test -- --coverage",
    "test:watch": "pnpm run test -- --watch"
  },
  "prettier": "@4s1/eslint-config",
  "dependencies": {
    "@4s1/snow-white-shared": "4.1.0",
    "@nestjs/common": "8.1.1",
    "@nestjs/core": "8.1.1",
    "@nestjs/platform-express": "8.1.1",
    "@nestjs/swagger": "5.1.3",
    "axios": "0.23.0",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "dotenv": "10.0.0",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "mqtt": "4.2.8",
    "mysql": "2.18.1",
    "nodemailer": "6.7.0",
    "reflect-metadata": "0.1.13",
    "rxjs": "7.4.0",
    "source-map-support": "0.5.20",
    "swagger-ui-express": "4.1.6",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@4s1/eslint-config": "3.2.0",
    "@4s1/ts-config": "1.2.2",
    "@commitlint/cli": "13.2.1",
    "@commitlint/config-conventional": "13.2.0",
    "@nestjs/cli": "8.1.4",
    "@nestjs/schematics": "8.0.4",
    "@nestjs/testing": "8.1.1",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.2",
    "@types/node": "14.17.27",
    "@types/nodemailer": "6.4.4",
    "@types/uuid": "8.3.1",
    "eslint": "8.0.1",
    "husky": "7.0.4",
    "jest": "27.3.1",
    "prettier": "2.4.1",
    "standard-version": "9.3.2",
    "ts-jest": "27.0.7",
    "ts-loader": "9.2.6",
    "ts-node": "10.3.0",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.4"
  },
  "engines": {
    "node": ">=14"
  }
}
