{
  "name": "@4s1/snow-white-rest-api",
  "version": "1.0.1",
  "description": "Snow White - Rest API",
  "bugs": {
    "url": "https://gitlab.com/4s1/snow-white-rest-api/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://gitlab.com/4s1/snow-white/snow-white-rest-api.git"
  },
  "license": "MIT",
  "author": "Steffen <steffen@4s1.de>",
  "scripts": {
    "prebuild": "rm -rf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\"",
    "preinstall": "npx only-allow pnpm",
    "lbt": "npm run lint && npm run build && npm run test",
    "lint": "eslint --ext .ts src/",
    "lint:fix": "npm run lint -- --fix",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand"
  },
  "prettier": "@4s1/eslint-config",
  "dependencies": {
    "@4s1/snow-white-shared": "4.0.1",
    "@nestjs/common": "7.6.18",
    "@nestjs/core": "7.6.18",
    "@nestjs/platform-express": "7.6.18",
    "@nestjs/swagger": "4.8.2",
    "@prisma/client": "3.0.2",
    "axios": "0.21.4",
    "class-transformer": "0.4.0",
    "class-validator": "0.13.1",
    "dotenv": "8.6.0",
    "moment": "2.29.1",
    "moment-timezone": "0.5.33",
    "mqtt": "4.2.8",
    "mysql": "2.18.1",
    "nodemailer": "6.6.3",
    "reflect-metadata": "0.1.13",
    "rxjs": "6.6.7",
    "swagger-ui-express": "4.1.6",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@4s1/eslint-config": "3.1.2",
    "@nestjs/cli": "7.6.0",
    "@nestjs/schematics": "7.3.1",
    "@nestjs/testing": "7.6.18",
    "@types/express": "4.17.13",
    "@types/jest": "27.0.1",
    "@types/node": "14.17.16",
    "@types/nodemailer": "6.4.4",
    "@types/supertest": "2.0.11",
    "@types/uuid": "8.3.1",
    "@typescript-eslint/eslint-plugin": "4.31.1",
    "@typescript-eslint/parser": "4.31.1",
    "eslint": "7.32.0",
    "jest": "27.2.0",
    "prettier": "2.4.0",
    "prisma": "3.0.2",
    "supertest": "6.1.6",
    "ts-jest": "27.0.5",
    "ts-loader": "8.3.0",
    "ts-node": "9.1.1",
    "tsconfig-paths": "3.11.0",
    "typescript": "4.4.3"
  },
  "engines": {
    "node": ">=14"
  }
}
